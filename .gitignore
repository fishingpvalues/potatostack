# PotatoStack .gitignore

# Environment files (secrets)
.env
.env.*
.envrc

# Docker volumes
volumes/

# Logs
*.log
logs/
log/
.logs/

# Temporary files
*.tmp
*.swp
*.swo
*~

# OS files
.DS_Store
Thumbs.db

# IDE files
.vscode/
.idea/
*.iml
.history/

# Backup files
*.bak
*.backup
*.tar.gz
backup-*.tar.gz

# FinTS Importer configurations (contains bank credentials)
config/fints-importer/*.json
config/fints-importer/*.xml
config/fints-importer/*.csv
!config/fints-importer/README.md
!config/fints-importer/.gitkeep

# Firefly III data
data/firefly/
