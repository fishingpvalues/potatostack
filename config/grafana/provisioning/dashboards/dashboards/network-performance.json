{
  "dashboard": {
    "id": null,
    "uid": "network-performance",
    "title": "Network & ISP Performance",
    "tags": ["network", "isp", "speedtest", "fritzbox", "latency", "sota-2025"],
    "timezone": "browser",
    "schemaVersion": 39,
    "version": 1,
    "refresh": "1m",
    "panels": [
      {
        "collapsed": false,
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 0},
        "id": 1,
        "panels": [],
        "title": "Internet Speed Test - Bandwidth & Latency",
        "type": "row"
      },
      {
        "datasource": "Prometheus",
        "description": "Download bandwidth from speedtest",
        "fieldConfig": {
          "defaults": {
            "unit": "bps",
            "decimals": 2,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "red"},
                {"value": 50000000, "color": "yellow"},
                {"value": 100000000, "color": "green"}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 1},
        "id": 2,
        "options": {
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "showThresholdLabels": false,
          "showThresholdMarkers": true
        },
        "pluginVersion": "11.0.0",
        "targets": [
          {
            "expr": "speedtest_download_bits_per_second{job=\"speedtest-exporter\"}",
            "legendFormat": "Download",
            "refId": "A"
          }
        ],
        "title": "Download Speed",
        "type": "gauge"
      },
      {
        "datasource": "Prometheus",
        "description": "Upload bandwidth from speedtest",
        "fieldConfig": {
          "defaults": {
            "unit": "bps",
            "decimals": 2,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "red"},
                {"value": 10000000, "color": "yellow"},
                {"value": 50000000, "color": "green"}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 1},
        "id": 3,
        "options": {
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "showThresholdLabels": false,
          "showThresholdMarkers": true
        },
        "pluginVersion": "11.0.0",
        "targets": [
          {
            "expr": "speedtest_upload_bits_per_second{job=\"speedtest-exporter\"}",
            "legendFormat": "Upload",
            "refId": "A"
          }
        ],
        "title": "Upload Speed",
        "type": "gauge"
      },
      {
        "datasource": "Prometheus",
        "description": "Speedtest ping latency over time",
        "fieldConfig": {
          "defaults": {
            "unit": "ms",
            "decimals": 2,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"},
                {"value": 50, "color": "yellow"},
                {"value": 100, "color": "red"}
              ]
            }
          }
        },
        "gridPos": {"h": 7, "w": 12, "x": 0, "y": 9},
        "id": 4,
        "options": {
          "legend": {"displayMode": "list", "placement": "bottom", "showLegend": true, "calcs": ["mean", "lastNotNull"]},
          "tooltip": {"mode": "multi", "sort": "none"}
        },
        "targets": [
          {
            "expr": "speedtest_ping_latency_milliseconds{job=\"speedtest-exporter\"}",
            "legendFormat": "Ping Latency",
            "refId": "A"
          }
        ],
        "title": "Speedtest Latency",
        "type": "timeseries"
      },
      {
        "datasource": "Prometheus",
        "description": "Bandwidth trend over time",
        "fieldConfig": {
          "defaults": {
            "unit": "bps",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"}
              ]
            }
          }
        },
        "gridPos": {"h": 7, "w": 12, "x": 12, "y": 9},
        "id": 5,
        "options": {
          "legend": {"displayMode": "table", "placement": "bottom", "showLegend": true, "calcs": ["mean", "max", "min"]},
          "tooltip": {"mode": "multi", "sort": "desc"}
        },
        "targets": [
          {
            "expr": "speedtest_download_bits_per_second{job=\"speedtest-exporter\"}",
            "legendFormat": "Download",
            "refId": "A"
          },
          {
            "expr": "speedtest_upload_bits_per_second{job=\"speedtest-exporter\"}",
            "legendFormat": "Upload",
            "refId": "B"
          }
        ],
        "title": "Bandwidth Trend",
        "type": "timeseries"
      },
      {
        "collapsed": false,
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 16},
        "id": 6,
        "panels": [],
        "title": "Fritzbox Router Metrics",
        "type": "row"
      },
      {
        "datasource": "Prometheus",
        "description": "DSL line signal-to-noise ratio (SNR). Higher is better.",
        "fieldConfig": {
          "defaults": {
            "unit": "dB",
            "decimals": 1,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "red"},
                {"value": 10, "color": "yellow"},
                {"value": 20, "color": "green"}
              ]
            }
          }
        },
        "gridPos": {"h": 7, "w": 12, "x": 0, "y": 17},
        "id": 7,
        "options": {
          "legend": {"displayMode": "list", "placement": "bottom", "showLegend": true, "calcs": ["lastNotNull"]},
          "tooltip": {"mode": "multi", "sort": "none"}
        },
        "targets": [
          {
            "expr": "fritz_dsl_snr_down{job=\"fritzbox-exporter\"}",
            "legendFormat": "SNR Downstream",
            "refId": "A"
          },
          {
            "expr": "fritz_dsl_snr_up{job=\"fritzbox-exporter\"}",
            "legendFormat": "SNR Upstream",
            "refId": "B"
          }
        ],
        "title": "DSL Signal-to-Noise Ratio",
        "type": "timeseries"
      },
      {
        "datasource": "Prometheus",
        "description": "DSL line attenuation. Lower is better.",
        "fieldConfig": {
          "defaults": {
            "unit": "dB",
            "decimals": 1,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"},
                {"value": 40, "color": "yellow"},
                {"value": 60, "color": "red"}
              ]
            }
          }
        },
        "gridPos": {"h": 7, "w": 12, "x": 12, "y": 17},
        "id": 8,
        "options": {
          "legend": {"displayMode": "list", "placement": "bottom", "showLegend": true, "calcs": ["lastNotNull"]},
          "tooltip": {"mode": "multi", "sort": "none"}
        },
        "targets": [
          {
            "expr": "fritz_dsl_attenuation_down{job=\"fritzbox-exporter\"}",
            "legendFormat": "Attenuation Downstream",
            "refId": "A"
          },
          {
            "expr": "fritz_dsl_attenuation_up{job=\"fritzbox-exporter\"}",
            "legendFormat": "Attenuation Upstream",
            "refId": "B"
          }
        ],
        "title": "DSL Line Attenuation",
        "type": "timeseries"
      },
      {
        "datasource": "Prometheus",
        "description": "WAN/LAN network traffic through router",
        "fieldConfig": {
          "defaults": {
            "unit": "Bps",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 24},
        "id": 9,
        "options": {
          "legend": {"displayMode": "table", "placement": "right", "showLegend": true, "calcs": ["mean", "max", "lastNotNull"]},
          "tooltip": {"mode": "multi", "sort": "desc"}
        },
        "targets": [
          {
            "expr": "rate(fritz_wan_bytes_received_total{job=\"fritzbox-exporter\"}[5m])",
            "legendFormat": "WAN Receive",
            "refId": "A"
          },
          {
            "expr": "rate(fritz_wan_bytes_sent_total{job=\"fritzbox-exporter\"}[5m])",
            "legendFormat": "WAN Transmit",
            "refId": "B"
          },
          {
            "expr": "rate(fritz_lan_bytes_received_total{job=\"fritzbox-exporter\"}[5m])",
            "legendFormat": "LAN Receive",
            "refId": "C"
          },
          {
            "expr": "rate(fritz_lan_bytes_sent_total{job=\"fritzbox-exporter\"}[5m])",
            "legendFormat": "LAN Transmit",
            "refId": "D"
          }
        ],
        "title": "Router Network Traffic",
        "type": "timeseries"
      },
      {
        "collapsed": false,
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 32},
        "id": 10,
        "panels": [],
        "title": "Connection Status & Errors",
        "type": "row"
      },
      {
        "datasource": "Prometheus",
        "description": "Router uptime and connection status",
        "fieldConfig": {
          "defaults": {
            "unit": "dtdurations",
            "decimals": 0,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"}
              ]
            }
          }
        },
        "gridPos": {"h": 5, "w": 12, "x": 0, "y": 33},
        "id": 11,
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "textMode": "value_and_name"
        },
        "pluginVersion": "11.0.0",
        "targets": [
          {
            "expr": "fritz_uptime_seconds{job=\"fritzbox-exporter\"}",
            "legendFormat": "Router Uptime",
            "refId": "A"
          }
        ],
        "title": "Fritzbox Uptime",
        "type": "stat"
      },
      {
        "datasource": "Prometheus",
        "description": "DSL CRC errors and connection status",
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "decimals": 0,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"},
                {"value": 1, "color": "yellow"},
                {"value": 100, "color": "red"}
              ]
            }
          }
        },
        "gridPos": {"h": 5, "w": 12, "x": 12, "y": 33},
        "id": 12,
        "options": {
          "colorMode": "background",
          "graphMode": "area",
          "justifyMode": "auto",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "textMode": "value_and_name"
        },
        "pluginVersion": "11.0.0",
        "targets": [
          {
            "expr": "fritz_dsl_crc_errors_down{job=\"fritzbox-exporter\"}",
            "legendFormat": "CRC Errors Down",
            "refId": "A"
          },
          {
            "expr": "fritz_dsl_crc_errors_up{job=\"fritzbox-exporter\"}",
            "legendFormat": "CRC Errors Up",
            "refId": "B"
          }
        ],
        "title": "DSL CRC Errors",
        "type": "stat"
      },
      {
        "collapsed": false,
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 38},
        "id": 13,
        "panels": [],
        "title": "System Network Traffic - Node Exporter",
        "type": "row"
      },
      {
        "datasource": "Prometheus",
        "description": "Network receive/transmit rate from node exporter",
        "fieldConfig": {
          "defaults": {
            "unit": "Bps",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 39},
        "id": 14,
        "options": {
          "legend": {"displayMode": "table", "placement": "bottom", "showLegend": true, "calcs": ["mean", "max"]},
          "tooltip": {"mode": "multi", "sort": "desc"}
        },
        "targets": [
          {
            "expr": "sum(rate(node_network_receive_bytes_total{instance=\"$instance\",device!~\"lo\"}[5m]))",
            "legendFormat": "Receive",
            "refId": "A"
          },
          {
            "expr": "sum(rate(node_network_transmit_bytes_total{instance=\"$instance\",device!~\"lo\"}[5m]))",
            "legendFormat": "Transmit",
            "refId": "B"
          }
        ],
        "title": "Node Network I/O",
        "type": "timeseries"
      },
      {
        "datasource": "Prometheus",
        "description": "Network errors and drops",
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"},
                {"value": 1, "color": "yellow"},
                {"value": 100, "color": "red"}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 39},
        "id": 15,
        "options": {
          "legend": {"displayMode": "table", "placement": "bottom", "showLegend": true, "calcs": ["lastNotNull", "max"]},
          "tooltip": {"mode": "multi", "sort": "desc"}
        },
        "targets": [
          {
            "expr": "sum(rate(node_network_receive_errs_total{instance=\"$instance\",device!~\"lo\"}[5m]))",
            "legendFormat": "Receive Errors",
            "refId": "A"
          },
          {
            "expr": "sum(rate(node_network_transmit_errs_total{instance=\"$instance\",device!~\"lo\"}[5m]))",
            "legendFormat": "Transmit Errors",
            "refId": "B"
          },
          {
            "expr": "sum(rate(node_network_receive_drop_total{instance=\"$instance\",device!~\"lo\"}[5m]))",
            "legendFormat": "Receive Drops",
            "refId": "C"
          },
          {
            "expr": "sum(rate(node_network_transmit_drop_total{instance=\"$instance\",device!~\"lo\"}[5m]))",
            "legendFormat": "Transmit Drops",
            "refId": "D"
          }
        ],
        "title": "Network Errors & Drops",
        "type": "timeseries"
      }
    ],
    "templating": {
      "list": [
        {
          "name": "instance",
          "type": "query",
          "datasource": "Prometheus",
          "query": "label_values(node_uname_info, instance)",
          "multi": false,
          "includeAll": false,
          "refresh": 1,
          "sort": 1
        }
      ]
    },
    "time": {"from": "now-24h", "to": "now"},
    "timepicker": {
      "refresh_intervals": ["30s", "1m", "5m", "15m", "30m", "1h"],
      "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
    }
  }
}
